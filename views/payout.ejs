<body class="host-dashboard">

	<!-- start header -->
	<%- include mainMasterDashboard.ejs %>
	<!-- end header -->

	<!-- start main content -->
	<input type="hidden" value="<%= firstName %>" id="firstName">
	<input type="hidden" value="<%= pmcId %>" id="pmcId">
	<input type="hidden" value="<%= url %>" id="globalUrl">
	<section id="getstarted" class="no-transition">
		<div class="container">
			<div class="row">
				<div class="col-md-12  margin-20px-top margin-30px-bottom">


					<h3 class="text-capitalize text-dark-blue font-weight-300">Payout Settings</h3>


				</div>
			</div>

			<div class="row">


				<div class="col-md-8 margin-30px-bottom">

					<div class="panel panel-default">
						<div class="panel-heading">
							<h6 class="text-blue">Bank Account Info </h6>
						</div>
						<div class="panel-body">
							<form action="/">
								<div class="row">
									<div class="form-group">

										<div class="col-md-12">

											<p>When you receive a payment for a booking, that payment is called a
												"payout". Our secure payment system will
												direct deposit the amount into a bank account after reservation check
												in. The payment deposit may take 1-5 businesss
												days. You setup your bank account here. </p>


											<div id="Bank_Account"></div>

										</div>
									</div>
								</div>

							</form>

						</div>

					</div>



				</div>




				<div class="col-md-1"></div>
				<div class="col-md-3">

					<div class="sidebar-box">
						<h5 class="text-capitalize text-dark-blue font-weight-300">Profile Settings</h5>
						<ul class="no-margin-left no-padding-left margin-20px-top">
							<li><a href="./profile">Profile Details</a></li>
							<li class="active"><a href="./payout">Payout Settings</a></li>
							<li><a href="./notification">Notifications</a></li>
							<li><a href="./Sign_out" onclick="signOut()">Sign Out</a></li>
						</ul>
					</div>

					<div class="sidebar-box">
						<h5 class="text-capitalize text-dark-blue font-weight-300">Share the Love</h5>
						<p>Help us grow by telling your friends about Whimstay.</p>
						<button type="button" id="shareMail" class="btn btn-blue btn-rounded btn-large width-100"
							data-toggle="modal" data-target="#myModal-Share" >Share</button>
					</div>

					<div class="sidebar-box">
						<h5 class="text-capitalize text-dark-blue font-weight-300">Follow Us</h5>
						<div class="sidebar-social-icon">
							<a href="https://www.facebook.com/whimstay" class="facebook-bg-hvr" target="_blank"><i
									class="fab fa-facebook-f"></i></a>
							<a href="https://www.instagram.com/whimstay" class="instagram-bg-hvr" target="_blank"><i
									class="fab fa-instagram"></i></a>
							<a href="https://www.linkedin.com/company/whimstay" class="linkedin-bg-hvr"
								target="_blank"><i class="fab fa-linkedin-in"></i></a>

						</div>
					</div>


				</div>


			</div>


		</div>
	</section>
	<!--end main content -->

	<!-- start add account form lightbox-->
	<!-- Modal -->
	<div id="myModal-AddAccount" class="modal fade" role="dialog">
		<div class="modal-dialog">

			<!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>

				</div>

				<div class="modal-body">



					<div class="form-group">
						<label for="accTypeRadio">Account Type:</label>
						<div class="radio">
							<label><input type="radio" name="accType" id="accType" value="1" checked>Individual</label>
						</div>
						<div class="radio">
							<label><input type="radio" name="accType" id="accType" value="2">Company</label>
						</div>
					</div>

					<div class="form-group">

						<div class="checkbox">
							<label>
								<input type="checkbox" name="agreement" value="agreement" onclick="validatePayout()"
									id="agreement" /> By registering
								your account, you agree to our <a href="./terms_privacy" target="blank">Services
									Agreement</a> and the <a href="https://stripe.com/us/connect-account/legal"
									target="_blank">Stripe Connected Account Agreement</a>.<br />
								<span id="checkboxval" style="color:red; font-weight: 500"></span>
							</label>
						</div>
					</div>

					<button type="submit" id="checkAccType" class="btn btn-blue btn-rounded btn-large width-100">Start
						Registration</button>


				</div>

			</div>

		</div>
	</div>

