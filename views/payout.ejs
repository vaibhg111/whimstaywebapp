					document.getElementById("displayFirstName").value = result.firstName;
					document.getElementById("displayLastName").value = result.lastName;
					document.getElementById("displayEmail").value = result.email;
					document.getElementById("displayAddress").value = result.address;
					document.getElementById("displayAddress2").value = result.addressline1;
					document.getElementById("displayCity").value = result.city;
					document.getElementById("displaystate").value = result.state;
					document.getElementById("displayZip").value = result.zipcode;
					document.getElementById('displayCountry').value = result.country;
					document.getElementById('displayCurrency').value = result.currency;
					document.getElementById('displayType').value = result.type;
					document.getElementById('accountHolderType').value = result.accountHolderType;
					document.getElementById('displayRoutingNumber').value = result.banks[0].routingNumber;
					if (result.dateOfBirth != null) {
						$('#datetimepicker5').data('DateTimePicker').defaultDate(moment(result.dateOfBirth));
						var date = result.dateOfBirth;
					}
					if (result.status == 'verified') {
						document.getElementById("displayHide").style.display = "none";
						document.getElementById("displayFirstName").disabled = true;
						document.getElementById("displayLastName").disabled = true;
					}

				},
				error: function (jqXHR, textStatus, errorThrown) {
					console.log('update Stock error: ' + textStatus);
				}

			});
		}

