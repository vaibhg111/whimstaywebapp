	<script type="text/javascript">
		$(function () {
			var now = new Date();
		var years_ago_date = new Date(now.getFullYear() - 18, now.getMonth(), now.getDate());
			$('#datetimepicker3').datetimepicker(
				{
					format: 'MM/DD/YYYY',
					maxDate: years_ago_date,
					useCurrent: false,
					allowInputToggle: true,
					viewDate: years_ago_date
				}
			);
		});

		$('#BusinessDob').keydown(function (evnt) {
			if (evnt.originalEvent.keyCode === 8) {
				return true;
			} else {
				return false;
			}
		});

