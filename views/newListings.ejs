			function readAndPreview(i, file) {
				if (existingImages) {
					newImageCount = ++existingImages;
				} else {
					newImageCount = i;
				}

				var imageId = newImageCount;
				var ImageName = file.name;

				if (file.size > 20971520) { // make it in MB so divide by 1024*1024
					document.getElementById("msg-body").innerHTML = "Please select image size less than 20 MB";
					$('#dyna-popup').modal('show');
					//console.log('Please select image size less than 20 MB');
					return false;
				}

				if (!/\.(jpe?g|png|gif)$/i.test(file.name)) {
					return console.log(file.name + " is not an image");
				} // else...

				var reader = new FileReader();

				$(reader).on("load", function () {
					$preview.append("<div  id='" + "image" + imageId + "'  class='img-wrap col-lg-4 col-sm-4 col-xs-6   img-responsive'   style='width:200px' ><span class='fa fa-times-circle fa-lg'  id='" + "image" + imageId + "'  title='Delete Image'   onClick ='deleteImage(this)'></span> <img  class='thumbnail  img-responsive'  src='" + this.result + "'   /></div>");

