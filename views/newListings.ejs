				$('#guestRoom1').empty();
			}
		});

		$("input#my_select").keypress(function (event) {
			var code = event.charCode || event.which;
			if (code >= 32 && !/^\d+$/.test(String.fromCharCode(code))) {
				event.preventDefault();
			}
			$('#my_select1').empty();
		});

		function validate1(event) {
			var value = document.getElementById("noBaths").value;
			if (value === '....' || value === '...' || value === '..') {
				document.getElementById("noBaths").value = 0;
			}
			if (value.indexOf(".") == 1 || value.indexOf(".") == 2) {
				value = value.split(".");
				if (value[1] >= 10) {
					value[1] = value[1];
				} else if (value[1] == 5) {
					value[1] = value[1] + 0;
				} else {
					value[1] = value[1] + 1;
