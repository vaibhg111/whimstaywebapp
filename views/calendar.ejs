          selectable: true,
          defaultView: 'month',

          axisFormat: 'h:mm',
          columnFormat: {
            month: 'ddd',    // Mon
            week: 'ddd d', // Mon 7
            day: 'dddd M/d',  // Monday 9/7
            agendaDay: 'dddd d'
          },
          titleFormat: {
            month: 'MMMM yyyy', // September 2009
